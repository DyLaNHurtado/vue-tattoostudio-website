{
    "cleanUrls": true,
    "trailingSlash": false,
    "headers": [
      {
        "source": "/(.*).(js|css|png|jpg|jpeg|gif|svg|woff|woff2|ttf|eot|ico|gz)",
        "headers": [
          {
            "key": "Content-Encoding",
            "value": "gzip"
          },
          {
            "key": "Cache-Control",
            "value": "public, max-age=31536000, immutable"
          }
        ]
      }
    ],
    "builds": [
      {
        "src": "dist/index.html",
        "use": "@vercel/static"
      },
      {
        "src": "dist/assets/**/*",
        "use": "@vercel/static"
      }
    ]
  }
  